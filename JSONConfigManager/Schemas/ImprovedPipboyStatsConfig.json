{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"EnableHotkeyTabChanges": {
			"type": "boolean"
		},
		"PerksTitle": {
			"type": "string"
		},
		"CurrentlyText": {
			"type": "string"
		},
		"HidePerksFromEffectsTab": {
			"type": "boolean"
		},
		"HidePerksFromEffectsTabExclude": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"HideOtherFromEffectsTab": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"MapPerkNames": {
			"type": "object",
			"patternProperties": {
				".*": {
					"type": "string"
				}
			}
		},
		"CurveDataDirectories": {
			"type": "object",
			"patternProperties": {
				".*": {
					"type": "array",
					"items": {
						"type": "string"
					}
				}
			}
		}
	}
